{
  "description": "To embed swagger right in the github ui.",
  "manifest_version": 2,
  "name": "Github Swagger UI Plugin",
  "version": "1.1.1",

  "browser_action": {
    "default_popup": "popup/Settings.html"
  },
  "icons": {
    "48": "logos/swagger-penguin-logo48.png"
  },

  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [
        "depend/jquery.js",
        "depend/swagger-ui.js",
        "swagger-plugin.js",
        "Script.js"
      ],
      "css" : [
        "depend/swagger-ui.css",
        "swagger-pluggin.css"
      ]
    }
  ]
}
